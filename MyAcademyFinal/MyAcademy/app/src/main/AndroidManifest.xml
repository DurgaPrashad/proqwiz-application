<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proqwiz">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.proqwiz.MyAccountActivity"></activity>
        <activity android:name="com.example.proqwiz.AnswersActivity" />
        <activity android:name="com.example.proqwiz.BookMarksActivity" />
        <activity android:name="com.example.proqwiz.LoginActivity" />
        <activity android:name="com.example.proqwiz.ScoreActivity" />
        <activity android:name="com.example.proqwiz.QuestionsActivity" />
        <activity android:name="com.example.proqwiz.StartTestActivity" />
        <activity android:name="com.example.proqwiz.TestsActivity" />
        <activity android:name="com.example.proqwiz.ResetPasswordActivity" />
        <activity android:name="com.example.proqwiz.SignUpActivity" />
        <activity android:name="com.example.proqwiz.MainActivity" />
        <activity android:name="com.example.proqwiz.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>