<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mtc.proadmin">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mtc.proadmin.AddBulkQActivity"></activity>
        <activity android:name="com.mtc.proadmin.QuestionDetailsActivity" />
        <activity android:name="com.mtc.proadmin.QuestionActivity" />
        <activity android:name="com.mtc.proadmin.TestActivity" />
        <activity android:name="com.mtc.proadmin.CategoryActivity" />
        <activity android:name="com.mtc.proadmin.MainActivity" />
        <activity android:name="com.mtc.proadmin.LoginActivity" />
        <activity android:name="com.mtc.proadmin.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>